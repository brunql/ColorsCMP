# Add inputs and outputs from these tool invocations to the build variables 
LSS += \
ColorsCMP_EclipseEdition.lss \

FLASH_IMAGE += \
ColorsCMP_EclipseEdition.hex \

SIZEDUMMY += \
sizedummy \


# All Target
all: ColorsCMP_EclipseEdition.elf secondary-outputs

# Tool invocations
ColorsCMP_EclipseEdition.elf: $(OBJS) $(USER_OBJS)
	@echo 'Building target: $@'
	@echo 'Invoking: AVR C++ Linker'
	avr-g++ -Wl,-Map,ColorsCMP_EclipseEdition.map,--cref -mmcu=atmega8 -o"ColorsCMP_EclipseEdition.elf" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo 'Finished building target: $@'
	@echo ' '

ColorsCMP_EclipseEdition.lss: ColorsCMP_EclipseEdition.elf
	@echo 'Invoking: AVR Create Extended Listing'
	-avr-objdump -h -S ColorsCMP_EclipseEdition.elf  >"ColorsCMP_EclipseEdition.lss"
	@echo 'Finished building: $@'
	@echo ' '

ColorsCMP_EclipseEdition.hex: ColorsCMP_EclipseEdition.elf
	@echo 'Create Flash image (ihex format)'
	-avr-objcopy -R .eeprom -O ihex ColorsCMP_EclipseEdition.elf  "ColorsCMP_EclipseEdition.hex"
	@echo 'Finished building: $@'
	@echo ' '

sizedummy: ColorsCMP_EclipseEdition.elf
	@echo 'Invoking: Print Size'
	-avr-size --format=avr --mcu=atmega8 ColorsCMP_EclipseEdition.elf
	@echo 'Finished building: $@'
	@echo ' '

# Other Targets
clean:
	-$(RM) $(OBJS)$(C_DEPS)$(ELFS)$(FLASH_IMAGE)$(LSS)$(S_DEPS)$(CXX_DEPS)$(S_UPPER_DEPS)$(C++_DEPS)$(ASM_DEPS)$(CC_DEPS)$(CPP_DEPS)$(C_UPPER_DEPS)$(SIZEDUMMY) ColorsCMP_EclipseEdition.elf
	-@echo ' '

secondary-outputs: $(LSS) $(FLASH_IMAGE) $(SIZEDUMMY)

